FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat csv;

interpolationScheme cellPoint;

fields
(
    R
    RB
    nut
    omega
);

sets
(

    X0H
    {
        type        face;
        axis        y;

        start       (0.05 0 0);
        end         (0.05 5 0);
    }

    X1H
    {
        type        face;
        axis        y;

        start       (1 0 0);
        end         (1 5 0);
    }

    X2H
    {
