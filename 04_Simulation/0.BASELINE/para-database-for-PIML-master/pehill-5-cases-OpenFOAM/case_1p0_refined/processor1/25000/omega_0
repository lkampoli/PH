{
    bottomWall
    {
        type            omegaWallFunction;
        beta1           0.075;
        blended         0;
        value           nonuniform 0();
    }
    defaultFaces
    {
        type            empty;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    topWall
    {
        type            omegaWallFunction;
        beta1           0.075;
        blended         0;
        value           nonuniform 0();
    }
