FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "20000";
    object      nu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 9.438414e-05;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    top
    {
        type            calculated;
        value           uniform 9.438414e-05;
    }
    hills
    {
        type            calculated;
        value           uniform 9.438414e-05;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


